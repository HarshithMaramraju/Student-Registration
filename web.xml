<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>registorURL</servlet-name>
		<servlet-class>SignUp.Registor</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>registorURL</servlet-name>
		<url-pattern>/registor</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>loginURL</servlet-name>
		<servlet-class>SignUp.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginURL</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
</web-app>
